            <div class="clinic-points">
              <% if (center.centerDetails.centerServices.length > 0) { %>
              <div>
                <img src="../clinic-panel/images/icons/done.png" alt="Icon Done" />
              </div>
              <% }else{ %>
              <div class="circle"></div>
              <% } %>
              <div>
                <div class="clinic-point-title">Hastalarınıza Sağladığınız İmkanları Belirtin</div>
                <% if (center.centerDetails.centerServices.length > 0) { %>
                <div class="clinic-point-process done">Bitti!</div>
                <% } %>
              </div>
            </div>

            <hr class="custom" />

            <div class="clinic-points">
              <% if (center.address.addressDetailText != undefined) { %>
              <div>
                <img src="../clinic-panel/images/icons/done.png" alt="Icon Done" />
              </div>
              <% }else{ %>
              <div class="circle"></div>
              <% } %>
              <div>
                <div class="clinic-point-title">Merkeziniz İçin Yol Tarifi Ekleyin</div>
                <% if (center.centerDetails.centerServices.length > 0) { %>
                <div class="clinic-point-process done">Bitti!</div>
                <% }else{ %>
                <div class="clinic-point-process">Hastalarınızın merkezinizi bulmasına yardımcı olun.</div>
                <% } %>
              </div>
            </div>

            <hr class="custom" />

            <!-- <div class="clinic-points">
              <div class="circle"></div>
              <div>
                <div class="clinic-point-title">Merkezinize Yakın Lokasyonlar Hakkında Bilgi Verin</div>
                <div class="clinic-point-process">Lütfen merkezinizin çevresinde yer alan lokasyonları ekleyin.</div>
              </div>
            </div>
            <hr class="custom" /> -->

            <div class="clinic-points">
              <% if (center.companyInformation.photo.length > 4) { %>
              <div>
                <img src="../clinic-panel/images/icons/done.png" alt="Icon Done" />
              </div>
              <% }else{ %>
              <div class="circle"></div>
              <% } %>

              <div>
                <div class="clinic-point-title">En Az 5 Adet Fotoğraf Yükleyin</div>
                <% if (center.companyInformation.photo.length > 4) { %>
                <div>
                  <div class="clinic-point-process done">Bitti!</div>
                </div>
                <% }else{ %>
                <div class="clinic-point-process">Şu an sadece <%- center.companyInformation.photo.length %> fotoğraf yüklediniz.</div>
                <% } %>
              </div>
            </div>
            <hr class="custom" />

            <div class="clinic-points">
              <% if (center.services.languages.length > 0) { %>
              <div>
                <img src="../clinic-panel/images/icons/done.png" alt="Icon Done" />
              </div>
              <% }else{ %>
              <div class="circle"></div>
              <% } %>
              <div>
                <div class="clinic-point-title">Çalışanlarınızın Konuştuğu Dilleri Ekleyin</div>
                <% if (center.services.languages.length > 0) { %>
                <div>
                  <div class="clinic-point-process done">Bitti!</div>
                </div>
                <% }else{ %>
                <div class="clinic-point-process">Çalışanlarınızın konuştuğu dilleri ekleyerek hastalarınızı bilgilendirin.</div>
                <% } %>
              </div>
            </div>
            <hr class="custom" />

            <div class="clinic-points">
              <% if (center.contactInformation.email != undefined && center.contactInformation.phone != undefined) { %>
              <div>
                <img src="../clinic-panel/images/icons/done.png" alt="Icon Done" />
              </div>
              <% }else{ %>
              <div class="circle"></div>
              <% } %>
              <div>
                <div class="clinic-point-title">İletişim Bilgilerinizi Güncelleyin</div>
                <% if (center.contactInformation.email != undefined && center.contactInformation.phone != undefined) { %>
                <div>
                  <div class="clinic-point-process done">Bitti!</div>
                </div>
                <% }else{ %>
                <div class="clinic-point-process">İletişim bilgilerinizi doldurun.</div>
                <% } %>
              </div>
            </div>
            <hr class="custom" />
            <div class="clinic-points">
              <% if (center.companyInformation.bio != undefined) { %>
              <div>
                <img src="../clinic-panel/images/icons/done.png" alt="Icon Done" />
              </div>
              <% }else{ %>
              <div class="circle"></div>
              <% } %>
              <div>
                <div class="clinic-point-title">Merkez Bio’nuzu Ekleyin</div>
                <% if (center.companyInformation.bio != undefined) { %>
                <div>
                  <div class="clinic-point-process done">Bitti!</div>
                </div>
                <% }else{ %>
                <div class="clinic-point-process">Hastalarınızı merkeziniz hakkında bilgilendirin.</div>
                <% } %>
              </div>
            </div>
            <!-- <hr class="custom" />

            <div class="clinic-points">
              <div class="circle"></div>
