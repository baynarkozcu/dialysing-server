<div class="main-panel">
  <div class="content-wrapper">
    <div id="section-i" class="tm-p">
      <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-6 col-xl-6">
          <div class="tm-section-title">İletişim Bilgileri</div>
          <form class="tm-form-wide">
            <div class="form-group">
              <label for="phone">Merkezinizin Telefon Numarasını Giriniz</label>
              <input type="tel" class="form-control" id="phone" placeholder="Telefon Numarası" />
            </div>
            <div class="form-group">
              <label for="phone">Alternatif Telefon Numarası Giriniz</label>
              <input type="tel" class="form-control" id="phone" placeholder="Telefon Numarası" />
            </div>
            <div class="form-group">
              <label for="email">Merkezinizin İletişim Mailini Girininiz</label>
              <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Email Adresiniz" />
            </div>
            <div class="form-group">
              <label for="email-repeat">Alternatif Mail Adresinizi Giriniz</label>
              <input type="email" class="form-control" id="email-repeat" aria-describedby="emailHelp" placeholder="Email Adresiniz" />
            </div>
            <div class="form-group">
              <label for="website">Websitesi</label>
              <input type="text" class="form-control" id="website" placeholder="Websitesi" />
            </div>
            <div class="form-group">
              <label for="address">Adres Bilgileri</label>
              <input type="text" class="form-control" id="address" placeholder="Adres Bilgileri" />
            </div>
            <button type="submit" class="btn-second">Değişiklikleri Kaydet</button>
          </form>
        </div>
      </div>
    </div>
  </div>
  <!-- content-wrapper ends -->
  <div id="suggestion">
    <div class="container">
      <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
          <div class="suggestion-button">
            <button type="button" class="btn-second suggestion"><img src="../clinic-panel/images/icons/suggestion.png" alt="Icon Suggestion" />Öneriler</button>
          </div>
        </div>
      </div>
    </div>
  </div>
  
  <%- include("../../partials/footer") %>
  <!-- partial -->
</div>
