        <input class="form-check-input" type="checkbox" value="Tekerlekli Sandalye Kullanım" name="centerServices" id="Tekerlekli_Sandalye_Kullanim" />
        <label class="form-check-label selected" for="Tekerlekli_Sandalye_Kullanim"> Tekerlekli Sandalye Kullanım </label>
      </div>
      <div class="form-group not-border checkbox">
        <input class="form-check-input" type="checkbox" value="Tutunma Kollu Tuvalet" name="centerServices" id="Tutunma_Kollu_Tuvalet" />
        <label class="form-check-label selected" for="Tutunma_Kollu_Tuvalet"> Tutunma Kollu Tuvalet </label>
      </div>
