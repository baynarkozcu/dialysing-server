<div class="col-sm-12 col-md-12 col-lg-3 col-xl-3">
  <div class="tm-box-categories">
    <div class="tm-categories-title">Filtreleme</div>
    <form class="tm-form-small filter" action="/clinic/clinic-list?city=<%- city  %>" method="POST">
      <div class="form-group">
        <label for="distance" class="form-label">Aranılan lokasyona uzaklık</label>
        <div id="distance" class="distance">
          <output id="result">30</output>
          <span>km uzaklık</span>
        </div>

        <input onchange="changeValue(this.value)" type="range" value="30" min="0" max="50" oninput="this.nextElementSibling.value = this.value" class="form-range" id="distance" />

        <div class="total-distance">Total: 50</div>
      </div>
      <div class="tm-categories-subtitle">Merkez Ortalama Puanı</div>
      <div class="form-group not-border checkbox">
        <input class="form-check-input" type="checkbox" value="Süper" name="centerPoint" id="Super" />
        <label class="form-check-label selected" for="Super"> Süper +8 </label>
      </div>
      <div class="form-group not-border checkbox">
        <input class="form-check-input" type="checkbox" value="Çok iyi" name="centerPoint" id="Cok_iyi" />
        <label class="form-check-label selected" for="Cok_iyi"> Çok İyi +7 </label>
      </div>
      <div class="form-group not-border checkbox">
        <input class="form-check-input" type="checkbox" value="İyi" name="centerPoint" id="Iyi" />
        <label class="form-check-label selected" for="Iyi"> İyi +6 </label>
      </div>
      <div class="form-group not-border checkbox">
        <input class="form-check-input" type="checkbox" value="Ortalama" name="centerPoint" id="Ortalama" />
        <label class="form-check-label selected" for="Ortalama"> Ortalama +5 </label>
