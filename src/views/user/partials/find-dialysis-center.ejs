<div class="col-sm-12 col-md-12 col-lg-3 col-xl-3">
  <div class="tm-box-categories">
    <div class="tm-categories-title">Diyaliz Merkezi Bul</div>
    <form class="tm-form-small">
      <div class="form-group">
        <label for="location">Lokasyon</label>
        <input type="text" class="form-control" id="location" placeholder="Lokasyon" />
      </div>
      <div class="form-group">
        <label for="check-in">Check-in Tarihi</label>
        <input type="date" class="form-control datepicker" id="check-in" placeholder="Check-in Tarihi" />
        <img src="../user/img/icons/down.png" alt="Icon Down" />
      </div>
      <div class="form-group">
        <label for="check-out">Check-out Tarihi</label>
        <input type="date" class="form-control datepicker" id="check-out" placeholder="Check-out Tarihi" />
        <img src="../user/img/icons/down.png" alt="Icon Down" />
      </div>
      <div class="form-group not-border checkbox">
        <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" />
        <label class="form-check-label selected" for="flexCheckChecked"> Çıkış tarihim belli değil </label>
      </div>
      <button class="btn btn-second wide small">Kayıt Ol</button>
    </form>
  </div>
</div>
